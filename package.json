{
  "name": "@raiffeisen-ecom/payment-sdk",
  "version": "1.1.6",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "clean": "cross-env rimraf lib lib-style dist/payment.* es",
    "lint": "cross-env NODE_PATH=. node ./node_modules/eslint/bin/eslint.js ./src",
    "prepare": "npm run clean && npm run build",
    "build": "npm run build:commonjs && npm run build:styled:commonjs && npm run build:iife && npm run build:iife:min && npm run build:iife:styled:min",
    "build:commonjs": "cross-env STYLE_EXTRACT=true FORMAT=cjs NODE_ENV=production BABEL_ENV=commonjs rollup -c -o lib-style/index.js",
    "build:styled:commonjs": "cross-env BABEL_ENV=commonjs FORMAT=cjs NODE_ENV=production rollup -c -o lib/index.js",
    "build:iife": "cross-env NODE_ENV=development rollup -c -o dist/payment.styled.js",
    "build:iife:styled:min": "cross-env STYLE_EXTRACT=false NODE_ENV=production rollup -c -o dist/payment.styled.min.js",
    "build:iife:min": "cross-env STYLE_EXTRACT=true STYLE_EXTRACT=true NODE_ENV=production rollup -c -o dist/payment.min.js"
  },
  "files": [
    "lib",
    "lib-style",
    "dist"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-private-methods": "^7.4.4",
    "@babel/plugin-syntax-class-properties": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-stage-0": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-globals": "^3.0.0",
    "codecov": "^3.5.0",
    "create-react-class": "^15.6.3",
    "cross-env": "^5.2.0",
    "es3ify": "^0.2.0",
    "eslint": "^6.2.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "glob": "^7.1.4",
    "jest": "^24.8.0",
    "jest-dom": "^3.5.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.14.6",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-css-only": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.0.0"
  },
  "author": "",
  "license": "MIT"
}
